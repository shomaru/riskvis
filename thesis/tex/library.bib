journal = {2009 International Conference on Advances in Social Network Analysis and Mining},
pages = {261--266},
publisher = {Ieee},
title = {{Integrated Representation and Visualisation of the Dynamics in Computer-mediated Social Networks}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5231871},
year = {2009}
}
